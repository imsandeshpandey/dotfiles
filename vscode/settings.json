        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },

  // APC (AESTHETIC PROJECT CONTROL) /////////////////////////////////
  "apc.header": {
    "height": 36
  },
  "apc.sidebar.titlebar": {
    "fontSize": 10,
    "height": 24
  },
  "apc.electron": {
    "titleBarOverlay": {
      "color": "#1a1a19",
      "symbolColor": "white",
      "height": 40
    }
  },
  "apc.listRow": {
    "lists": ["explorer-folders-view", "quick-input-list"],
    "height": 32
  },
  "apc.imports": ["file:///E:/vscode_settings/vsc.css"],

  // EXTENSION-SPECIFIC SETTINGS  ///////////////////////////////////
  "symbols.hidesExplorerArrows": true,
  "tree-generator.exclude": {
    "node_modules": true,
    ".git": true
  },
  "leetcode.workspaceFolder": "c:\\Users\\Administrator\\code",
  "leetcode.defaultLanguage": "typescript",
  "cmake.options.statusBarVisibility": "visible",
  "supermaven.enable": {
    "*": true
  },

  // NEOVIM INTEGRATION  /////////////////////////////////////////////
  "vscode-neovim.neovimExecutablePaths.linux": "/home/linuxbrew/.linuxbrew/bin/nvim",
  "vscode-neovim.wslDistribution": "ubuntu",
  "vscode-neovim.useWSL": true,
  "vscode-neovim.ctrlKeysForInsertMode": [
    "a",
    "d",
    "h",
    "j",
    "m",
    "o",
    "r",
    "t",
    "u",
    "w"
  ],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "b",
    "d",
    "e",
    "f",
    "h",
    "i",
    "j",
    "k",
    "l",
    "m",
    "o",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "z",
    "/",
    "]",
    "right",
    "left",
    "up",
    "down",
    "backspace",
    "delete"
  ],

  //  GITHUB COPILOT  ////////////////////////////////////////////////
  "github.copilot.advanced": {
    "useLanguageServer": true
  },

  //  MISCELLANEOUS  ////////////////////////////////////////////////
  "update.mode": "default",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.activityBar.location": "bottom",
  "window.menuBarVisibility": "toggle",
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "security.workspace.trust.untrustedFiles": "open",
  "security.allowedUNCHosts": ["wsl.localhost"],
  "breadcrumbs.enabled": false
}
