{
  // EDITOR CORE SETTINGS /////////////////////////////////////////////////
  "editor.fontSize": 14.1,
  "editor.fontFamily": "monolisa",
  "editor.lineHeight": 2.4,
  "editor.lineNumbers": "relative",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.fontLigatures": true,
  "editor.accessibilitySupport": "off",
  "editor.acceptSuggestionOnEnter": "off",

  // VISUAL INTERFACE ///////////////////////////////////////////////////
  "workbench.colorTheme": "Everforest Dark",
  "workbench.iconTheme": "symbols",
  "workbench.productIconTheme": "feather-vscode",
  "workbench.activityBar.orientation": "vertical",
  "workbench.activityBar.iconClickBehavior": "focus",

  // WINDOW SETTINGS ///////////////////////////////////////////////////
  "window.zoomLevel": 0,
  "window.title": "${activeRepositoryName}",

  // SCROLLING AND NAVIGATION ///////////////////////////////////////////
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.minimap.enabled": true,
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,

  // FILE MANAGEMENT ////////////////////////////////////////////////////
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,

  // FILE NESTING //////////////////////////////////////////////////////
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.config.ts, ${capture}.config.js, ${capture}.config.json",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.config.ts, ${capture}.config.js, ${capture}.config.json",
    "*.jsx": "${capture}.js, ${capture}.config.ts, ${capture}.config.js, ${capture}.config.json",
    "*.tsx": "${capture}.ts ${capture}.*.tsx ${capture}.config.ts ${capture}.config.js ${capture}.config.json",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "tailwind.config.*": "tailwind.config*, postcss.config*",
    ".env": ".env*"
  },

  // LANGUAGE SERVERS //////////////////////////////////////////////////
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[plsql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "[lua]": {
    "editor.defaultFormatter": "notnoobmaster.lua-beautifier"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },

  // FONT SETTINGS //////////////////////////////////////////////////
  "editor.codeLensFontFamily": "default",
  "editor.inlayHints.fontFamily": "default",
  "chat.editor.fontFamily": "inter",
  "markdown.preview.fontFamily": "\"Inter\",-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "scm.inputFontFamily": "Inter",
  "scm.inputFontSize": 14,
  "apc.font.family": "inter medium",
  "terminal.integrated.fontFamily": "JetBrains Mono",

  // GIT & SCM  //////////////////////////////////////////////////////
  "git.decorations.enabled": false,
  "scm.diffDecorations": "none",

  // FRAMEWORK & LANGUAGE SUPPORT  ////////////////////////////////////
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tailwindCSS.includeLanguages": {
    "html": "html"
  },

  // TODO TREE CONFIGURATION  ///////////////////////////////////////
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "FUNC",
    "GROUP",
    "EDITABLE"
  ],
  "todo-tree.highlights.customHighlight": {
    "EDITABLE": {
      "icon": "pencil",
      "type": "line",
      "gutterIcon": true,
      "background": "#7E78D2",
      "foreground": "#fff",
      "iconColour": "#7E78D2"
    },
    "GROUP": {
      "icon": "three-bars",
      "type": "line",
      "gutterIcon": true,
      "background": "#fff",
      "foreground": "#000",
      "iconColour": "#fff"
    },
    "FUNC": {
      "icon": "three-bars",
      "type": "line",
      "gutterIcon": true,
      "background": "#70CAD1",
      "foreground": "#000",
      "iconColour": "#70CAD1"
    },
    "FIXME": {
      "foreground": "#000",
      "iconColour": "#fff",
      "gutterIcon": true
    }
  },

